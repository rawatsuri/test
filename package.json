{
  "name": "express-typescript-prisma-postgresql",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "prebuild": "prisma generate",
    "build": "node -e \"require('fs').rmSync('dist', { recursive: true, force: true })\" && tsc && tsc-alias",
    "start": "node dist/server.js",
    "dev": "nodemon --ext ts --exec tsx src/server.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest --pool=forks --maxWorkers=1 --minWorkers=1",
    "test:ci": "vitest --run --pool=forks --maxWorkers=1 --minWorkers=1",
    "verify": "npm run lint && npm run format:check && npm run test:ci",
    "postinstall": "prisma generate"
  },
  "keywords": [
    "Express",
    "Typescript",
    "Prisma",
    "PostgreSQL"
  ],
  "author": "Sushant Rahate",
  "license": "MIT",
  "description": "",
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.9",
    "@types/pino": "^7.0.4",
    "@types/pino-http": "^5.8.4",
    "@types/pino-pretty": "^4.7.5",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "@vitest/coverage-v8": "^3.0.8",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^57.0.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "prisma": "^6.19.2",
    "supertest": "^7.2.2",
    "tsc-alias": "^1.8.11",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8"
  },
  "dependencies": {
    "@clerk/clerk-sdk-node": "^5.1.6",
    "@clerk/express": "^1.7.69",
    "@prisma/client": "^6.19.2",
    "axios": "^1.13.5",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "fs-extra": "^11.3.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
    "svix": "^1.84.1",
    "uni-response": "^1.0.3",
    "zod": "^3.24.2"
  }
}
